// dot -Tpng digraph.gv -o digraph.png
digraph G {
      main -> parse -> execute;
      main -> init;
      main -> cleanup;
      execute -> make_string;
      execute -> printf
      init [style="bold"]
      init -> make_string;
      main -> printf;
      execute -> compare;
      compare [style=filled]
}